{
    "rewrites": [
      {
        "source": "/admin",
        "destination": "/admin/index.html"
      },
      {
        "source": "/admin/(.*)",
        "destination": "/admin/index.html"
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/404.html",
        "status": 404
      }
    ],
    "buildCommand": "npm run build:vercel",
    "outputDirectory": "dist",
    "framework": "astro"
  }